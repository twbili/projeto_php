<form method="post" action="{{ path('app_admin_news_delete', {'id': news.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ news.id) }}">

    <button class="btn btn-danger text-white float-start me-3 ">Deletar</button>
</form>
</form>
