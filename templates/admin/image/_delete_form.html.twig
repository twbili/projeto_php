<form method="post" action="{{ path('app_admin_image_delete', {'id': image.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ image.id) }}">

    <button class="btn btn-danger text-white float-start me-3 ">Deletar</button>
</form>
